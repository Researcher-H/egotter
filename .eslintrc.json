{
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2019
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "$": true,
    "ga": true,
    "Highcharts": true,
    "d3": true,
    "Egotter": true,
    "Snackbar": true,
    "SnackMessage": true,
    "ToastMessage": true,
    "Mustache": true,
    "AsyncLoader": true
  },
  "rules": {
    "semi": "error"
  }
}
